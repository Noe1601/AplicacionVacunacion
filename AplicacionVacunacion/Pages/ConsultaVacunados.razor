@page "/ConsultaVacunados"

<h3>Consulta Vacunados</h3>
<hr />

<div class="buscar mb-5">
    <form>
        <div class="form-group">
            <input class="form-control" placeholder="Ingrese nombre de quien desea consultar" id="nombre" />
        </div>

        <button type="submit" class="btn btn-outline-success">Buscar</button>
    </form>
</div>

<div class="container">
    <table class="table table-bordered">

        <thead class="thead-dark">
            <tr>
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Fecha nacimiento</th>
                <th>Telefono</th>
                <th>Cantidad de vacunas</th>
            </tr>
        </thead>

        <tbody>
            <tr>
                
            </tr>
        </tbody>

    </table>
</div>

